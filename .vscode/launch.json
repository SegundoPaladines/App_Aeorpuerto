{
    "configurations": [
        {
            "type": "java",
            "name": "Launch AppAeropuerto",
            "request": "launch",
            "mainClass": "Main.AppAeropuerto",
            "projectName": "Aeropuerto"
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "vmArgs": "--module-path ${env:PATH_TO_FX} --add-modules javafx.controls",
            "mainClass": "${file}"
        }
    ]
}